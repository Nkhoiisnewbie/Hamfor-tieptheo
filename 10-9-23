#include <stdio.h>
int main()
{
	int n,i=1,s=0;
	printf("Nhap n: ");
	do
	{

		scanf("%d",&n);
		if(n<10||n>100)
		printf("Vui long nhap lai n tu 10 den be hon 100: ");
	}
	while(n<10||n>100);
	{
//a.	
	i++;
	if(n%i==0)
	{
	printf("n la so nguyen to");
	}
					
	else 
	
	printf("n khong phai la so nguyen to");
	
		return 1;
	}
	
}
